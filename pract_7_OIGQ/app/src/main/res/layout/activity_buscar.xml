<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Buscar">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="110dp"
        android:layout_height="41dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:autoSizeTextType="uniform"
        android:text="Buscar"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.017" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="168dp"
        android:layout_height="19dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Nombre del encuestado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/et2"
        android:layout_width="361dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ScrollView
        android:id="@+id/scrollView3"
        android:layout_width="352dp"
        android:layout_height="291dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et2"
        app:layout_constraintVertical_bias="0.034">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv9"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:text="Nombre"
                android:textSize="24sp"
                tools:layout_editor_absoluteX="162dp"
                tools:layout_editor_absoluteY="164dp" />

            <TextView
                android:id="@+id/tv10"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:textColor="@color/colorPrimary"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="162dp"
                tools:layout_editor_absoluteY="199dp" />

            <TextView
                android:id="@+id/tv11"
                android:layout_width="360dp"
                android:layout_height="45dp"
                android:text="Diria que esta aplicacion ha curado su disfuncion erectil?"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="162dp"
                tools:layout_editor_absoluteY="228dp" />

            <TextView
                android:id="@+id/tv12"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:textColor="@color/colorPrimary"
                android:textSize="18sp"
                tools:layout_editor_absoluteX="162dp"
                tools:layout_editor_absoluteY="261dp" />

            <TextView
                android:id="@+id/tv13"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:text="Pagaria por esta aplicacion"
                android:textSize="22sp"
                tools:layout_editor_absoluteX="162dp"
                tools:layout_editor_absoluteY="287dp" />

            <TextView
                android:id="@+id/tv14"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:textColor="@color/colorPrimary"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:text="あなたはタコスが好きですか"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/tv16"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:textColor="@color/colorPrimary"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="340dp"
                android:layout_height="45dp"
                android:text="Cuanto le daria a esta aplicacion del 1 al 100"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv18"
                android:layout_width="360dp"
                android:layout_height="40dp"
                android:textColor="@color/colorPrimary"
                android:textSize="18sp" />

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:onClick="consulta"
        android:text="Buscar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView3" />

</android.support.constraint.ConstraintLayout>